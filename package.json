{
  "name": "require-extension-hooks-vue",
  "version": "0.3.0",
  "description": "Simple server parser for .vue files",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "ava": {
    "files": [
      "spec/**/*.spec.js"
    ],
    "source": [
      "index.js"
    ],
    "require": [
      "./spec/helpers/setup.js"
    ],
    "verbose": false,
    "concurrency": 8,
    "timeout": "240s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackmellis/require-extension-hooks-vue.git"
  },
  "author": "Jack Ellis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jackmellis/require-extension-hooks-vue/issues"
  },
  "homepage": "https://github.com/jackmellis/require-extension-hooks-vue#readme",
  "peerDependencies": {
    "require-extension-hooks": "0.x"
  },
  "dependencies": {
    "consolidate": "^0.14.5",
    "parse5": "^3.0.2",
    "vue-template-compiler": "^2.2.0",
    "vue-template-es2015-compiler": "^1.5.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "browser-env": "^2.0.31",
    "pug": "^2.0.0-rc.1",
    "require-extension-hooks": "^0.2.0",
    "require-extension-hooks-babel": "^0.1.0",
    "sinon": "^2.2.0",
    "vue": "^2.3.2"
  }
}
